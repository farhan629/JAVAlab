package javaapplication1;

import java.util.Scanner;

/**
 *
 * @author 22cseb45
 */
public class NewClass3 {
    public static void main(String args[]){
        Scanner s=new Scanner(System.in);
        System.out.println("Enter how much number :");
        int a=s.nextInt();
        for(int i=1;i<=a;i++){
            System.out.println(i);
        }
    }
    
}







