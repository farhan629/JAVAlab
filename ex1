package javaapplication1;

import java.util.*;

/**
 *
 * @author 22cseb45
 */
public class NewClass4 {
    public static void main(String args[]){
     Scanner s=new Scanner(System.in);
     System.out.println("Enter how much number :");
     int a=s.nextInt();
     for(int i=1;i<=a;i++){
         for(int j=1;j<=i;j++){
            System.out.print(j+" ");

         }
                 System.out.println();

}
}}



















